    var crypto_array = ["IE", "IE", "IE", "IE", "IE", "IE"];
    var fx_array = ["EUR", "USD", "CHF", "GBP", "JPY", "AUD", "CAD", "NZD"];
    let rand_live_trade_time = Math.floor(Math.random() * 2000 + 500);

    let crypto_rand = Math.floor(Math.random() * 6 + 0);
    let fx_rand = Math.floor(Math.random() * 8 + 0);
    let country_rand = Math.floor(Math.random() * 14 + 0);
    let firstname_rand = Math.floor(Math.random() * 50 + 0);
    let lastname_rand = Math.floor(Math.random() * 20 + 0);
    let letter_rand = Math.floor(Math.random() * 24 + 0);

    let rand_profit = Math.floor(Math.random() * 90000 + 1000);
    rand_profit = rand_profit / 100;

    var dt = new Date();
    var hours = dt.getHours();
    var minutes = dt.getMinutes();
    var seconds = dt.getSeconds();
    if (hours < 10) {
        hours = "0" + hours;
    }
    if (minutes < 10) {
        minutes = "0" + minutes;
    }
    if (seconds < 10) {
        seconds = "0" + seconds;
    }
    var current_time = hours + ":" + minutes + ":" + seconds;
    let rand_profit_final = "$" + rand_profit.toFixed(0);

    setTimeout(function () {
        let country_code_rand = country_array[country_rand];
        $(".live-trades").prepend(
            '<tr><td><span class="flag-icon flag-icon-' +
            country_code_rand +
            ' flag-icon-squared"></span></td><td>' +
            firstname_array[country_code_rand][firstname_rand] +
            " " +
            letter_array[letter_rand] +
            '.</td><td><div class=" button-3d button-mini button-rounded button-green nomargin">' +
            rand_profit_final +
            '</div></td><td class="live-trade-opt"><div class="live-trade-currency  button-3d button-mini button-rounded button-white button-light nomargin">' +
            crypto_array[crypto_rand] +
            "/" +
            fx_array[fx_rand] +
            '</div></td><td class="live-trade-opt"><div class="live-trade-time  button-3d button-mini button-rounded button-white button-light nomargin">' +
            current_time +
            "</div></td></tr>"
        );
    }, rand_live_trade_time);

    setTimeout(function () {
        liveTrade();
    }, rand_live_trade_time);
}

function spotsLeft() {
    var number = parseInt('43');
    var interval = setInterval(function () {
        $('.spotsLeft').text(number--);
        if (number == 0) {
            clearInterval(interval);
        }
    }, 18000);
}

spotsLeft();
liveTrade();
// initRandomUsers();

$(".js-range-slider").ionRangeSlider({
    skin: "round",
    min: 250,
    max: 500000,
    from: 250,
    prefix: '$',
    grid: !0,
    onStart: function (n) {
        $("#calcResult").text('$' + Math.round(.32 * n.from + n.from).toLocaleString("en-US"))
    },
    onChange: function (n) {
        $("#calcResult").text('$' + Math.round(.32 * n.from + n.from).toLocaleString("en-US"))
    }
});

const links = document.querySelectorAll('a[href="#"]');
links.forEach(function (e) {
    e.addEventListener('click', function (evt) {
        evt.preventDefault();
        document.querySelector('.hero__form form').scrollIntoView({
            block: 'center',
            behavior: 'smooth'
        });

    });
});





